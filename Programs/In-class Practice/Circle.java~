public class Circle {
  
  /* instance variables */
  private double radius;
  
  /* Default Contructor */
  public Circle() {
    /* Initialize instance variables */
    radius = 0;
  }
  
  /*Overloaded constructor */
  public Circle(double radiusIn) {
    radius = radiusIn;
  }
  
}